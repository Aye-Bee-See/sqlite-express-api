{
	"name": "abc-4",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"test": "NODE_ENV=test SUPPRESS_SEED_LOGS=1 mocha --timeout 10000 | grep -v 'Executing ' | tee testresults.txt",
		"test:api": "mocha test/api.test.js --timeout 10000",
		"test:markdown": "NODE_ENV=test SUPPRESS_SEED_LOGS=1 mocha --timeout 10000 --reporter spec | node format-test-results.js",
		"test:enhanced": "NODE_ENV=test SUPPRESS_SEED_LOGS=1 mocha --timeout 10000 --reporter spec | node enhanced-formatter.js",
		"test:report": "npm run test:enhanced && node html-preview.js && echo 'Test reports generated: testresults.md and testresults.html'",
		"lint": "eslint --fix \"**/*.+(js|mjs)\"",
		"format": "prettier --write \"**/*.+(js|mjs|json|css|md)\"",
		"pre-commit": "lint-staged",
		"prepare": "husky install"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"**/*.+(js|mjs)": "eslint --fix",
		"**/*.+(js|mjs|json|css|md)": "prettier --write"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"bcrypt": "^5.1.1",
		"body-parser": "^1.20.2",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^5.0.1",
		"jsonwebtoken": "^9.0.2",
		"multer": "^2.0.1",
		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
		"passport-local": "^1.0.0",
		"sequelize": "^6.37.2",
		"sqlite3": "^5.1.7"
	},
	"imports": {
		"#/*": [
			"./*",
			"./*.js",
			"./*.mjs",
			"./*.cjs"
		],
		"#db/*": [
			"./database/*",
			"./database/*.js",
			"./database/*.mjs",
			"./database/*.cjs"
		],
		"#hooks/*": [
			"./database/hooks/*",
			"./database/hooks/*.js",
			"./database/hooks/*.mjs",
			"./database/hooks/*.cjs"
		],
		"#models/*": [
			"./database/models/*",
			"./database/models/*.js",
			"./database/models/*.mjs",
			"./database/models/*.cjs"
		],
		"#schemas/*": [
			"./database/schemas/*",
			"./database/schemas/*.js",
			"./database/schemas/*.mjs",
			"./database/schemas/*.cjs"
		],
		"#seeds/*": [
			"./database/seeds/*",
			"./database/seeds/*.js",
			"./database/seeds/*.mjs",
			"./database/seeds/*.cjs"
		],
		"#routes/*": [
			"./routes/*",
			"./routes/*.js",
			"./routes/*.mjs",
			"./routes/*.cjs"
		],
		"#rtControllers/*": [
			"./routes/controllers/*",
			"./routes/controllers/*.js",
			"./routes/controllers/*.mjs",
			"./routes/controllers/*.cjs"
		],
		"#rtServices/*": [
			"./routes/services/*",
			"./routes/services/*.js",
			"./routes/services/*.mjs",
			"./routes/services/*.cjs"
		],
		"#services/*": [
			"./services/*",
			"./service/*.js",
			"./services/*.mjs",
			"./services/*.cjs"
		],
		"#dbg/*": [
			"./debug/*.mjs"
		],
		"#constants": "./constants.js"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.9",
		"@eslint/css": "^0.8.1",
		"@eslint/js": "^9.28.0",
		"@eslint/json": "^0.12.0",
		"@eslint/markdown": "^6.4.0",
		"chai": "^5.2.0",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.4.1",
		"globals": "^16.2.0",
		"husky": "^8.0.0",
		"lint-staged": "^16.1.1",
		"mocha": "^11.6.0",
		"nodemon": "^3.1.0",
		"prettier": "3.5.3",
		"quick-lint-js": "3.2.0",
		"supertest": "^7.1.1"
	}
}
